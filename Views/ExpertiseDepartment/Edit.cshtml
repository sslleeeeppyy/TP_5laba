@model TP_2laba.Models.ExpertiseDepartment
@{
    ViewData["Title"] = "Редактировать отдел экспертизы";
}
<h2>@ViewData["Title"]</h2>
<form asp-action="Edit" method="post">
    @Html.HiddenFor(m => m.Id)
    <div class="form-group">
        @Html.LabelFor(m => m.Name)
        @Html.EditorFor(m => m.Name)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.HeadFullName)
        @Html.EditorFor(m => m.HeadFullName)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.EmployeeCount)
        @Html.EditorFor(m => m.EmployeeCount)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.FoundationDate)
        @Html.EditorFor(m => m.FoundationDate)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Email)
        @Html.EditorFor(m => m.Email)
    </div>
    <button type="submit" class="btn btn-primary">Сохранить</button>
</form> 