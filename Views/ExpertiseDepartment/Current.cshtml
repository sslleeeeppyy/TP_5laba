@model TP_2laba.Models.ExpertiseDepartment
@{
    ViewData["Title"] = "Текущий экземпляр отдела экспертизы";
}
<h2>@ViewData["Title"]</h2>
@if (Model != null)
{
    <div>
        @Html.LabelForModel()
        @Html.DisplayForModel(Model)
    </div>
    <p>
        <a asp-action="Index">Назад к списку</a>
    </p>
}
else
{
    <p>Нет текущего экземпляра.</p>
    <a asp-action="Index">Назад к списку</a>
} 